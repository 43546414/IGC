{% extends 'partials/base.html.twig' %}

{% macro pageLinkName(text) %}{{ text|lower|replace({' ':'-'}) }}{% endmacro %}

{% block content %}
    {% for module in page.collection() %}
        <div id="{{ _self.pageLinkName(module.menu) }}">
          {{ module.content }}
        </div>
    {% endfor %}
{% endblock %}
